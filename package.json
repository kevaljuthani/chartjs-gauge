    "test": "karma start karma.conf.js",
    "test:html": "karma start karma.conf.js --reporters progress,kjhtml",
    "clone-chartjs": "git clone --branch v2.8.0 --depth 1 https://github.com/chartjs/Chart.js.git modules/chart.js"
